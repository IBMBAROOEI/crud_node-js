<div class="page-data">
  <div class="data-btn">
 
  </div>
  <div class="data-table">
      <table>
          <tr>
              <th width="50px">No</th>
              <th>Name</th>
              <th>edit</th>
              <th>del</th>
          </tr>

          <% if(data.length){
          for(var i = 0;i < data.length;i++) { %>
          <tr>
              <td><%=(i+1)%></td>
              <td><%=data[i].name%></td>
            
              <td>
                  <a class="a-inside edit" href="../list/edit/<%=data[i].id%>">Edit</a>
              
                  <a class="a-inside delete" href="../list/delete/<%=data[i].id%>">Delete</a>
              </td>
          </tr>
          <% }
          }else{ %>
          <tr>
              <td colspan="6">No post</td>
          </tr>
          <% } %>
      </table>
  </div>
</div>